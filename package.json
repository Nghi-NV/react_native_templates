{
  "name": "react_native_templates",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "debug": "REACT_DEBUGGER=\"rndebugger-open --open --port 8081\" yarn start",
    "reset": "watchman watch-del-all && rm -rf $TMDIR/react-* && rm -rf node_modules && yarn cache clean",
    "start:reset": "yarn start --reset-cache",
    "generate": "plop --plopfile internals/generators/index.js"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "react": "16.3.1",
    "react-native": "^0.55.4",
    "react-native-device-info": "^0.21.4",
    "react-native-localization": "^1.0.4",
    "react-native-material-kit": "^0.5.1",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-smart-splash-screen": "^2.3.5",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.9",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-persist-immutable": "^4.3.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.3",
    "node-plop": "0.7.0",
    "plop": "1.8.0",
    "react-test-renderer": "^16.3.0-alpha.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts"
    ]
  }
}
